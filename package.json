{
  "name": "@soulsolidity/registry",
  "version": "0.0.1-test.0",
  "description": "SoulSolidity Registry Types and Utilities",
  "license": "MIT",
  "author": "Doublo <doublo54@gmail.com>",
  "repository": "https://github.com/SoulSolidity/registry.git",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./lib ./data",
    "prebuild": "yarn lint",
    "build:tsc": "tsc",
    "build:scripts": "ts-node scripts/build.ts",
    "build": "yarn clean && yarn build:tsc && yarn build:scripts",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "check": "yarn format:check && yarn lint",
    "test": "echo \"No tests specified yet\""
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "keywords": [
    "typescript",
    "registry",
    "solidity",
    "blockchain"
  ],
  "publishConfig": {
    "access": "public"
  }
}
